  },
  {
    "key": "ctrl+alt+j",
    "when": "inStickySelectionMode",
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "line",
      "value": 5,
      "select": true
    }
  },

  {
    "key": "alt+k",
